import random

def light(value=0):
	d = [0,1]
	vkl = 0
	vikl = 0
	for i in range(0,100):
		value = random.choice(d)
		if value == 0:
			print('свет выключен')
			vkl += 1
		else: print('Свет включен'); vikl +=1
	print(vkl)
	print(vikl)
def chench(a):
	if a == 1:
		return 0
	else: return 1
try:
	light()
	while True:
		
		light(chench(input(':')))
except:
	None
